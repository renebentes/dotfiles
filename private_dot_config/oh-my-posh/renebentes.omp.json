{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:violet",
          "foreground": "p:white",
          "leading_diamond": "\ue0b6",
          "properties": {
            "debian": "\uf306",
            "linux": "\ue712",
            "macos": "\ue711",
            "raspbian": "\uf315",
            "ubuntu": "\uf31c",
            "windows": "\ue70f"
          },
          "style": "diamond",
          "type": "os"
        },
        {
          "background": "p:violet",
          "foreground": "p:white",
          "style": "diamond",
          "template": " {{ .UserName }}@{{ .HostName }} ",
          "trailing_diamond": "\ue0b0",
          "type": "session"
        },
        {
          "background": "p:light_blue",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "style": "folder"
          },
          "style": "powerline",
          "type": "path"
        },
        {
          "background": "p:yellow",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:light_yellow{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:light_orange{{ end }}",
            "{{ if gt .Ahead 0 }}p:light_purple{{ end }}",
            "{{ if gt .Behind 0 }}p:orange{{ end }}"
          ],
          "foreground": "p:black",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "style": "powerline",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#5c2d91",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue7b0 {{ .Context }}",
          "type": "docker"
        },
        {
          "background": "#5c2d91",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }}",
          "type": "dotnet"
        },
        {
          "background": "#777bb4",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue73d {{ .Full }} ",
          "type": "php"
        },
        {
          "background": "#6ca35e",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uf898 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "background": "#cc3a3a",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "type": "npm"
        },
        {
          "background": "#c3002f",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "type": "angular"
        },
        {
          "type": "status",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#00897b",
          "background_templates": ["{{ if gt .Code 0 }}#e91e63{{ end }}"],
          "trailing_diamond": "\ue0b4",
          "template": " {{ if gt .Code 0 }}\uf525{{ else }}\uf469{{ end }} ",
          "properties": {
            "always_enabled": true
          }
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{.Folder}}{{if .Root}} :: root{{end}} :: {{.Shell}}",
  "final_space": true,
  "palette": {
    "black": "#282a36",
    "red": "#e91e63",
    "light_blue": "#61afef",
    "light_orange": "#f26d50",
    "light_purple": "#b388ff",
    "light_yellow": "#ffeb95",
    "orange": "#f17c37",
    "violet": "#6272a4",
    "white": "#f8f8f2",
    "yellow": "#2fda4e"
  },
  "version": 3
}
